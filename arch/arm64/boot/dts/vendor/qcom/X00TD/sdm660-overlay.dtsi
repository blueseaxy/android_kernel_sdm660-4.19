/ {
cpus {
	cpu@0 {
		efficiency = <636>;
		capacity-dmips-mhz = <636>;
	};

	cpu@4 {
		efficiency = <1024>;
		capacity-dmips-mhz = <1024>;
	};
};

	energy_costs: energy-costs {
		compatible = "sched-energy";

		CPU_COST_0: core-cost0 {
			busy-cost-data = <
				 633600	 31
				 902400	 52
				1113600	 76
				1401600	119
				1536000	146
				1747200	197
				1843200	216
			>;
		};

		CPU_COST_1: core-cost1 {
			busy-cost-data = <
				1113600	244
				1401600	341
				1747200	513
				1958400	660
				2150400	808
				2208000	997
			>;
		};
	};

	reserved-memory {
		ramoops {
			compatible = "removed-dma-pool", "ramoops";
			no-map;
			reg = <0x0 0x9ff00000 0x0 0x00400000>;
			console-size = <0x200000>;
			pmsg-size = <0x200000>;
			status = "ok";
		};
		
		cont_splash_mem: splash_region@9d400000 {
			reg = <0x0 0x9d400000 0x0 0x02400000>;
		};

		/delete-node/ dfps_data_mem@0x9f7ff000;

		dfps_data_mem: dfps_data_mem@0x9f800000 {
			reg = <0x0 0x9f800000 0x0 0x00001000>;
			label = "dfps_data_mem";
		};
	};
};

&firmware {
	android {
		vbmeta {
			status = "disabled";
		};

		fstab {
			vendor {
				type = "ext4";
				mnt_flags = "ro,barrier=1,discard";
				fsmgr_flags = "wait";
			};

			system {
				compatible = "android,system";
				dev = "/dev/block/platform/soc/c0c4000.sdhci/by-name/system";
				type = "ext4";
				mnt_flags = "ro,barrier=1,discard";
				fsmgr_flags = "wait";
				status = "ok";
			};
		};
	};
};

&wdog {
	qcom,bark-time = <20000>;

};

&msm_gpu {
	/* sdm636.dtsi defaults to A509, but X00T has A512 tweaks */
	qcom,chipid = <0x05010200>;
};

&mem_client_3_size {
	/delete-property/ qcom,allocate-on-request;
	qcom,allocate-boot-time;
};

&soc {
	qcom,glink-smem-native-xprt-adsp@86000000 {
		/delete-property/ cpu-affinity;
	};
	
	ufs1: ufshc@1da4000 {
		/delete-property/ pinctrl-names;
		/delete-property/ pinctrl-0;
		/delete-property/ pinctrl-1;
	};

	gpio_keys {
		/delete-node/ camera_focus;
		/delete-node/ camera_snapshot;

		vol_up {
			gpios = <&tlmm 44 0x1>;
		};

		vol_down {
			label = "volume_down";
			gpios = <&tlmm 43 0x1>;
			linux,input-type = <1>;
			linux,code = <114>;
			gpio-key,wakeup;
			debounce-interval = <15>;
		};
	};

	qcom,msm_fastrpc {
		/delete-property/ qcom,fastrpc-adsp-sensors-pdr;
	};
};

&thermal_zones {
	/delete-node/ vbat_adc;
	/delete-node/ vbat_low;
	/delete-node/ vbat_too_low;
};
